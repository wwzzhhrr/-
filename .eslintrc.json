{
  "extends": ["prettier", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "requireConfigFile": false,
    "babelOptions": {
      "presets": [
        "@babel/preset-react"
      ]
    }
  },
    "parser": "@babel/eslint-parser",
    "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off"
  }
}